		};
		sio_audio: sio@e804e800 {
			compatible = "hisilicon,hi3630-sio";
			/* hisilicon,is_master; */
			status = "ok";
		};
		sio_voice: sio@e804ec00 {
			compatible = "hisilicon,hi3630-sio";
			/* hisilicon,is_master; */
			status = "ok";
		};
		sio_bt: sio@e804f000 {
			compatible = "hisilicon,hi3630-sio";
			/* hisilicon,is_master; */
			status = "disabled";
		};
		sio_modem: sio@e804f400 {
			compatible = "hisilicon,hi3630-sio";
